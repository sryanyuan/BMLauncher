QDialog#Dialog{
	background-image:url(:/images/dlgbk.jpg);
}

Dialog QLabel#m_pLblTitle{
	color:white;
	font-family:Times New Roman;
	font-size:16;
	font-weight:bold;
}

Dialog QPushButton#m_pBtnClose{
	border-image:url(:/images/systembutton.png) 0 98 0 387 stretch;
	border:none;
}

Dialog QPushButton#m_pBtnClose:hover{
	border-image:url(:/images/systembutton.png) 0 49 0 49 stretch;
}

Dialog QPushButton#m_pBtnClose:pressed{
	border-image:url(:/images/systembutton.png) 0 0 0 98 stretch;
}